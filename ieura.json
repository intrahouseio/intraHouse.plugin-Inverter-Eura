{
  "name": "ieura",
  "description": "Eura E1000 inverter RS485-net - TCP client via MOXA",
  "module": "ieura.js",
  "single": 0,

  "params": [
    {
      "name": "host",
      "note": "IP узла",
      "type": "string",
      "val": "192.168.0.250"
    },
    {
      "name": "port",
      "note": "Порт узла",
      "type": "number",
      "val": "4001"
    }
  ],

  "chpattern": [
    {
      "name": "desc",
      "note": "Type",
      "type": "droplist",
      "data": [{ "id": "INV", "name": "Частотник" },{ "id": "ELC", "name": "Ток" },{ "id": "ELV", "name": "Напряжение" }]
    }
  ],

  "chdefault": {
    "chan": "inv_1",
    "desc": "INV"
  },

  "genrules": {
    "INV": {
      "type": "520",
      "cl": "ActorA",
      "calc":"Math.round(60*(value/100)/(2*1))",
      "calc_out":"Math.round(value*100/(60/(2*1)))",

      "ext": [
        { "name": "current", "note": "Ток", "type": "number", "op": "r" },
        { "name": "volt", "note": "Напряжение", "type": "number", "op": "r" },

        { "name": "ratio", "note": "Отношение", "type": "number", "op": "r" },
        { "name": "errstr", "note": "Ошибка", "type": "string", "op": "r" }
      ]
    },

    "ELC": {
      "type": "200",
      "cl": "SensorA"
    },

    "ELV": {
      "type": "200",
      "cl": "SensorA"
    }
  },

  "actions": [{}]
}
